{
    "name": "coastercms/framework",
    "description": "Coaster CMS Framework for Laravel 8",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Neil Jennings",
            "email": "neilj.cardiff@gmail.com"
        },
        {
            "name": "Daniel Chadwick",
            "email": "chadanuk+coastercms@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^2.6|^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "~8.0",
        "laravelcollective/html": "~6.2",
        "bkwld/croppa": "~4.10",
        "blueimp/jquery-file-upload": "9.19.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "mockery/mockery": "^1.4.2",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "CoasterCms\\": "src/",
            "CoasterCmsDatabase\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "CoasterCms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoasterCms\\CmsServiceProvider"
            ]
        }
    }
}
